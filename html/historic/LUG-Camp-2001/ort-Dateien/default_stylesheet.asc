
body {
    background-color: #FAF9F6;
    color: #000000;
    font-family: Verdana, Arial, Helvetica, sans-serif;
    font-size: 10pt;
}

a:link {
    color: #0000AA;
    text-decoration: none;
}

a:visited {
    color: #000088;
    text-decoration: none;
}

a:active {
    color: #000044;
    text-decoration: underline;
}

a:hover {
    color:  #000044;
    text-decoration: underline;
}

p {
    font-family: Verdana, Arial, Helvetica, sans-serif;
}

h1 {
    font-size:  140%;
    font-family: Verdana, Arial, Helvetica, sans-serif; 
    font-weight:  bold;
    color:  #000000;
}

h2 {
    font-size:  115%;
    font-family: Verdana, Arial, Helvetica, sans-serif; 
    background-color: #FAF9F6;
    color:  #000000;
}

h3 {
    font-size:  110%;
    font-family: Verdana, Arial, Helvetica, sans-serif;
    background-color: #FAF9F6;
    color:  #000000;
}

td,th {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    text-align: left;
}

ul {
    font-family: Verdana, Arial, Helvetica, sans-serif;
}


.invisible {
  font-size: 80%;
  color: #FAF9F6;
}

.mild {
    color: #7f7f7f;
}

.DesktopStatusBar{
  font-size: 100%;
  font-family: Arial, Verdana, Helvetica, sans-serif;
  color: #ff0000;
}

table {
    border: 0;
}

h1.PortalTitle {
    background-color:  #FAF9F6;
    color: #000000;
    font-family: Arial, Verdana, Helvetica, sans-serif;
    font-size: 120%;
    font-weight: bold;
}


td.BoxTitle  {
   font-family: Verdana, Arial, Helvetica, sans-serif; 
   font-weight: bold;
   text-align:center; 
}

table.Box {
    font-family: Verdana, Arial, Helvetica, sans-serif;
    background-color: #F0F0F0;
}

table.Box tr td {
    font-family: Verdana, Arial, Helvetica, sans-serif; 
    font-size: 90%;
}

table.Box tr td.ActionTitle {
    font-family: Verdana, Arial, Helvetica, sans-serif; 
    font-weight: bold
}

table.Box tr td a:visited {
    color: #0000AA;
}

td.Desktop {
    vertical-align: top;
}

td.Desktop table tr {
    vertical-align: top;
}

div.Desktop p {
    font-size: 100%;
    font-family: Verdana, Arial, Helvetica, sans-serif;
}

div.Desktop h1 {
    font-size: 120%;
    margin-right:100pt;
    font-family: Arial, Verdana, Helvetica, sans-serif;
}

div.Desktop p.ModificationStatus { 
    font-size: 90%; 
    font-family: Verdana, Arial, Helvetica, sans-serif; 
}

div.AuthWarning {
    text-align: center;
    font-style: italic;
}

div.AuthWarning table {
    border: 0;
}

div.AuthWarning tr.Hot {
    color: #FF0000;
}

div.Error {
    color:  #FF0000;
}


table.FormLayout {
    width: 80%;
}

table.FormLayout tr {
    vertical-align: top;
}

table.FormLayout tr th.TextField {
    vertical-align: top;
}

table.FormLayout tr td.TextField {
    vertical-align: top;
}

table.FormLayout th {
    text-align: right;
}

table.FormLayout dl.FieldHelp dd {
    font-size: 70%;
}

table.ContentsList {
}

table.ContentsList tr td img {
    border: 0;
}

table.SearchResults {
    width: auto;
}

table.SearchResults tr th {
    text-align: left;
}

table.SearchResults tr td img {
    border: 0;
}

table.Wizard {
    width: auto;
}

table.Wizard tr {
    vertical-align: top;
}

table.Wizard tr th {
    text-align: right;
}

div.NewsBar {
    text-align: right;
}

table.NewsItems {
    border: 0;
    font-family: Verdana, Arial, Helvetica, sans-serif;
}


td.NewsBorder {
    background-color: #FAF9F6;
}

td.NewsListing {
    background-color: #FAF9F6;
}

.NewsLeadin {
    background-color: #FAF9F6;
 
}

.NewsByLine {
    background-color:  #FAF9F6;
}

.NewsDateline {
    background-color:  #FAF9F6;
}

td.NewsTitle {
    background-color: #FAF9F6;
    color: #000000;
    text-align: center;
    font-size: 90%;
    font-weight: bold;
}

tr.NewsItemRow td {
    background-color: #AAAAAA;
    font-size: 70%;
}

p.NewsHeadline {
    background-color:  #FAF9F6;
}
td.ListName {
    background-color:  #FAF9F6;
    font-weight: bold;
}
td.ListDefinition {
    font-style: italic;
}
